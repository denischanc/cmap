/*******************************************************************************
@TEST_DESC@ Call console info/error fns
*******************************************************************************/

@FUNCTION(test)
{
  LOCAL test = function()/t/{
    cmap.console.info("Hello "/t/, "World "/t/, "!!!"/t/).info();
    cmap.console.info();

    cmap.console.error("[ERROR]"/t/);
  };

  LOCAL job = new cmap.scheduler.job{test}/t/;
  job.schedule();
}

/*******************************************************************************
*******************************************************************************/

[C[

int main(int argc, char * argv[])
{
  cmap_bootstrap(NULL);
  cmap_env_main(cmap_env(), argc, argv, test);
  return cmap_main();
}

]]
