/*******************************************************************************
@TEST_DESC@ Call console info/error fns
*******************************************************************************/

[CMAP[

LOCAL aisleT = "aisle.t"#global#;

LOCAL test = function()##aisleT##{
  cmap.console.info("Hello "##aisleT##, "World "##aisleT##,  "!!!"##aisleT##)
    .info();
  cmap.console.info();

  cmap.console.error("[ERROR]"##aisleT##);
};

LOCAL job = new cmap.scheduler.job{test}#aisle.t#;
job.schedule();

]]
