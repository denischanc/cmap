/*******************************************************************************
@TEST_DESC@ Call console info/error fns
*******************************************************************************/

@FUNCTION(test)
{
  LOCAL aisleT = "aisle.t"#global#;

  LOCAL test = function()##aisleT##{
    cmap.console.info(
      "Hello "##aisleT##, "World "##aisleT##, "!!!"##aisleT##).info();
    cmap.console.info();

    cmap.console.error("[ERROR]"##aisleT##);
  };

  LOCAL job = new cmap.scheduler.job{test}#aisle.t#;
  job.schedule();
}

/*******************************************************************************
*******************************************************************************/

[C[

int main(int argc, char * argv[])
{
  cmap_bootstrap(NULL);
  cmap_env_main(cmap_env(), argc, argv, test);
  return cmap_main();
}

]]
