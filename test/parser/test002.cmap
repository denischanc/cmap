[CMAP[

local j = 0;

for(i = 0; i < 5; i = 0.valueOf(i).inc())
{
  j.add(i);
}

if(j != 10) { cmap.fatal("j != 10"); }
if(i != 5) { cmap.fatal("i != 5"); }

]]
